with "c_lib/c_libfswatch";

project Ada_Libfswatch is

   for Languages use ("Ada");
   for Source_Dirs use ("src", "generated");
   for Main use ("basic.adb") & project'Main;

   for Object_Dir use "obj";

   package Compiler is
      for Default_Switches ("Ada") use ("-g", "-O0");
   end Compiler;

end Ada_Libfswatch;
